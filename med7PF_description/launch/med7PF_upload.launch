<?xml version="1.0"?>
<launch>

    <!-- This launch file just loads the URDF of med7 with PF unit into the ROS Parameter Server -->
    <arg name="hardware_interface" default="PositionJointInterface"/> 
    <arg name="robot_name" default="lbr"/>
    <arg name="origin_xyz" default="'0 0 0'"/> <!-- Note the syntax to pass a vector -->
    <arg name="origin_rpy" default="'0 0 0'"/>

    <param name="robot_description" command="$(find xacro)/xacro '$(find med7PF_description)/urdf/med7PF.xacro' hardware_interface:=$(arg hardware_interface) robot_name:=$(arg robot_name) origin_xyz:=$(arg origin_xyz) origin_rpy:=$(arg origin_rpy)"/>
</launch>
